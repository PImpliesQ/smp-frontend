generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Recipe {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  name        String
  userId      String
  description String   @db.Text
  ingredients String   @db.Text
  steps       String   @db.Text
  diet        String
  people      Int
  foodSaved   Int
}
